# Example Environment Variables
# Copy this to .env.local for development or configure in Vercel for production

# TTS Server URL (Update after deploying to Render)
NEXT_PUBLIC_TTS_SERVER_URL=http://localhost:5001

# Groq API Configuration
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=llama-3.1-8b-instant

# MongoDB Configuration
MONGODB_URI=your_mongodb_uri_here

# SendGrid Email Configuration
SENDGRID_API_KEY=your_sendgrid_api_key_here
SENDGRID_FROM=no-reply@agrimater.com
